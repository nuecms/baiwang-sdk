{
  "name": "@nuecms/baiwang-sdk",
  "version": "0.0.16",
  "description": "百望云发票SDK",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "files": [
    "dist",
    "cjs",
    "esm",
    "./README.assets/",
    "README.md"
  ],
  "scripts": {
    "rollup:watch": "rollup -w -c",
    "ts:watch": "tsc -w -p tsconfig.esm.json & tsc -w -p tsconfig.cjs.json",
    "watch": "npm run rollup:watch & npm run ts:watch",
    "build": "rimraf esm && rimraf cjs && rimraf dist && tsc -p tsconfig.esm.json && tsc -p tsconfig.cjs.json && rollup -c",
  },
  "keywords": [
    "banu",
    "sdk",
    "baiwang",
    "invoice"
  ],
  "author": "Terry Cai",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "axios": "^1.4.0",
    "chalk": "^5.2.0",
    "crypto-js": "^4.1.1",
    "rimraf": "^5.0.1",
    "rollup": "^3.25.3",
    "rollup-plugin-dts": "^5.3.0",
    "typescript": "^5.1.5"
  }
}